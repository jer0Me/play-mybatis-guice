<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="daos.UsersDao">

    <select id="getUsers" flushCache="true" resultType="TmUser">

        SELECT *
        FROM tm_user

    </select>

    <select id="getUsersMap" flushCache="true" resultType="hashmap">

        SELECT *
        FROM tm_user

    </select>

</mapper>